{
  "name": "visual-insights",
  "version": "0.4.1",
  "author": "Observed Observer",
  "keywords": [
    "insights",
    "visualization"
  ],
  "main": "./build/cjs/index.js",
  "module": "./build/esm/index.js",
  "types": "./build/esm/index.d.ts",
  "dependencies": {
    "cube-core": "^2.13.0"
  },
  "scripts": {
    "rollup": "rollup -c ./config/rollup.config.cjs.js",
    "build": "npm run buildfront && npm run buildback",
    "buildback": "tsc -p ./tsconfig.cjs.json",
    "buildfront": "tsc -p ./tsconfig.esm.json",
    "test": "npm run rollup && mocha --no-timeouts",
    "coverage": "istanbul cover _mocha --report lcovonly -- --no-timeouts -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "coveralls": "^3.0.9",
    "istanbul": "^0.4.5",
    "mocha": "^6.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "rollup": "^1.29.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "rollup-plugin-uglify": "^6.0.4",
    "typescript": "^3.7.5"
  }
}
